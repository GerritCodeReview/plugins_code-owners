[access]
	inheritFrom = Public-Plugins
[submit]
	action = inherit
[access "refs/*"]
	owner = group Anonymous Users
	owner = group plugins-code-owners
[access "refs/heads/*"]
	label-Verified = -1..+1 group Anonymous Users
	label-Verified = -1..+1 group gerrit-verifiers
[project]
	description = The code-owners provides support for defining code owners for files in a repository. \n\nIf the code-owners plugin is enabled, changes can only be submitted if all touched files are covered by approvals from code owners.
[access "refs/for/*"]
	owner = group Anonymous Users
